        android:layout_alignParentLeft="true"
        android:textSize="18dp"
        android:textStyle="bold"
        />

    <TextView
        android:id="@+id/row_stage_startdescription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/row_stage_name"
        android:layout_alignLeft="@+id/row_stage_name"
        android:textSize="14dp"
        />

    <TextView
        android:id="@+id/row_stage_finishdescription"
